<!doctype html>
<html lang="pt-br">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Coding Challenge VTEX</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="/lib/bootstrap/bootstrap.css">
    <link rel="stylesheet" href="/style/main.css">
  </head>

  <body>
    <div class="container">
      <div class="row">
        <h1>Dev Shop</h1>
      </div>

      <div class="row">
        <h2>Developers</h2>

        <form class="form-inline">
          <input type="text" class="form-control" id="organization" placeholder="Organization">
          <button class="btn btn-primary search">Search</button>
        </form>

        <table class="table developers">
          <thead>
            <tr>
              <th>Photo</th>
              <th>Username</th>
              <th>Price</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
          </tbody>
        </table>
      </div>

      <div class="cart row">
        <h2>Cart</h2>
        <table class="table cart">
          <thead>
            <tr>
              <th>Username</th>
              <th>Price</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
          </tbody>
        </table>
      </div>

      <div class="totalizer row">
        <div class="col-sm-5">
          <div class="row">
            <table class="table">
              <tbody>
              </tbody>
            </table>
          </div>
        </div>
      </div>

    </div> <!-- /container -->

    <script>window.jQuery || document.write('<script src="/lib/jquery/jquery-1.11.2.min.js"><\/script>')</script>
    <script>
        var baseUrl = '<%=baseUrl%>';
    </script>
    <script src="/lib/bootstrap/bootstrap.js"></script>
    <script src="/script/main.js"></script>
  </body>
</html>
